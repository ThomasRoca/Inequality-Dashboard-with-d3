    "import pandas as pd\n",
    "import numpy as np\n",
    "import sys,os,os.path\n",
    "\n",
    "#Loading dataset\n",
    "data=\"Inequality_dashboard/WIID_ver3_sept15_0.xls\"  \n",
    "dataset = pd.read_excel(data)\n",
    "\n",
    "#Keep only the variable we want - including meta of interest - and get rid of missing values:\n",
    "dataset=dataset[[\"Country\",\"Gini\",\"Year\", \"Countrycode3\", \"Welfaredefn\", \"AreaCovr\"]]\n",
    "#get rid of rows containing missing values\n",
    "dataset=dataset.dropna()\n",
    "\n",
    "#empty dataframe to be fill in\n",
    "dataset_clean=pd.DataFrame(columns=[])\n",
    "\n",
    "#extract iso3 list, keep unique values with set(), record it in a list(), a sorted one..\n",
    "iso3list=sorted(list(set(dataset['Countrycode3'])))\n",
    "\n",
    "for iso3 in iso3list:\n",
    "    #look for the list of year available by iso3 store it in Year_list\n",
    "    Year_list=dataset[\"Year\"][dataset[\"Countrycode3\"] == iso3].tolist()\n",
    "    #keep only the data for the last year available\n",
    "    year_max=max(Year_list)\n",
    "    datasetUnique= dataset[(dataset[\"Countrycode3\"] == iso3) & (dataset[\"Year\"] == year_max) \\\n",
    "                           & (dataset[\"AreaCovr\"] != \"Rural\")\\\n",
    "                            & (dataset[\"AreaCovr\"] != \"Urban\")] \n",
    "    #append the data\n",
    "    dataset_clean = dataset_clean.append(datasetUnique)\n",
    "\n",
    "#Keep only the variable we need to plot the gini and sort them \n",
    "dataset_clean = dataset_clean[['Country', 'Gini', 'Year']]\n",
    "#set Gini as index and sort by it\n",
    "dataset_clean=dataset_clean.set_index(['Gini'])\n",
    "dataset_clean.sort_index(inplace=True)\n",
    "print(dataset_clean)\n",
    "\n",
    "#Save the dataset as tab separated data\n",
    "data=dataset_clean.to_csv(sep=\"\\t\", encoding=\"utf-8\", index=True, header=True)\n",
    "#open an handle\n",
    "f = open(\"Inequality_dashboard/data_temp.tsv\",'w')\n",
    "f.write(data)\n",
    "f.close()\n"
   ]
