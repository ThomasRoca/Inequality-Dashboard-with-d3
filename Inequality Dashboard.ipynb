    "#======> Cleaning the dataset:\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import sys,os,os.path\n",
    "\n",
    "#extract iso3 list, keep unique values with the set() command, record it in a list(), a sorted one.. with the sort() command\n",
    "iso3list=sorted(list(set(dataset['Countrycode3'])))\n",
    "print(iso3list)\n",
    "\n",
    "#create and empty dataframe that will host the selected data\n",
    "dataset_clean=pd.DataFrame(columns=[\"Country\", \"Countrycode3\", \"Year\", \"Incomegroup\", \\\n",
    "                 \"Source\", \"D1\", \"D2\", \"D3\", \"D4\", \"D5\", \"D6\", \"D7\", \"D8\", \"D9\", \"D10\", \\\n",
    "                                     \"Quality\", \"Welfaredefn\", \"AreaCovr\"])\n",
    "print(dataset_clean)\n",
    "\n",
    "for iso3 in iso3list:\n",
    "    #look for the list of year available by iso3 store it in Year_list\n",
    "    Year_list=dataset[\"Year\"][dataset[\"Countrycode3\"] == iso3].tolist()\n",
    "    #print(Year_list)\n",
    "    #store the min and max for these year\n",
    "    year_min=min(Year_list)\n",
    "    year_max=max(Year_list)\n",
    "    #print(iso3, year_min, year_max, len(Year_list))\n",
    "    \n",
    "    # for countries that have more than one data point in time, we record the data for year min and year max\n",
    "    if year_max != year_min :\n",
    "        datasetmin= dataset[(dataset[\"Countrycode3\"] == iso3) \\\n",
    "                            &(dataset[\"Year\"] == year_min) \\\n",
    "                            & (dataset[\"AreaCovr\"] != \"Rural\")\\\n",
    "                            & (dataset[\"AreaCovr\"] != \"Urban\")] #& (dataset[\"Welfaredefn\"] != \"Consumption\")]\n",
    "        dataset_clean = dataset_clean.append(datasetmin)\n",
    "\n",
    "        datasetmax= dataset[(dataset[\"Countrycode3\"] == iso3) \\\n",
    "                            & (dataset[\"Year\"] == year_max) \\\n",
    "                            & (dataset[\"AreaCovr\"] != \"Rural\") \\\n",
    "                            & (dataset[\"AreaCovr\"] != \"Urban\") ]#& (dataset[\"Welfaredefn\"] != \"Consumption\")]\n",
    "        dataset_clean = dataset_clean.append(datasetmax)\n",
    "        print('Record',iso3 ,\"year_min:\", year_min, \"year_max:\", year_max,'from year_max != year_min:')\n",
    "        continue\n",
    "        \n",
    "    # for countries that have only one data point in time, we record data for this point. \n",
    "    if year_max == year_min :\n",
    "        \n",
    "        #But.. certain countries can have different surveys for the same year.\n",
    "        #Here we can latter add specific features tho choose among sources, and other meta\n",
    "        # I finally withdraw this condition (dataset[\"Welfaredefn\"] != \"Consumption\") to increase the number of countries\n",
    "        # Will then need a manual cleaning step\n",
    "        \n",
    "        if len(Year_list)>1:\n",
    "            datasetUnique= dataset[(dataset[\"Countrycode3\"] == iso3) \\\n",
    "                                & (dataset[\"Year\"] == year_max)] #& (dataset[\"Welfaredefn\"] != \"Consumption\")]\n",
    "            dataset_clean = dataset_clean.append(datasetUnique)\n",
    "            print('Record',iso3 ,\"year_min:\", year_min, \"year_max:\", year_max,'from len(Year_list)>1:')\n",
    "            continue\n",
    "        \n",
    "        # easy one: only one data point for one year\n",
    "        if len(Year_list)==1:\n",
    "            datasetUnique= dataset[(dataset[\"Countrycode3\"] == iso3) \\\n",
    "                                & (dataset[\"Year\"] == year_max)] \n",
    "            dataset_clean = dataset_clean.append(datasetUnique)\n",
    "            print('Record',iso3 ,\"year_min:\", year_min, \"year_max:\", year_max,'from len(Year_list)==1:')\n",
    "            continue\n",
    "\n",
    "#save the results            \n",
    "dataset_clean.to_csv('Inequality_dashboard/dataset_clean.csv')"
